{{!-- const name = data.body.artists.items[0].name; --}}

{{#if data}}
    <div class="albums">
        {{#each data}}
            <div id="album">
                <img id="albumImage" src="{{this.images.0.url}}" alt="{{this.name}} image">
                <p><span>{{this.name}}</span></p>
                <a href="/tracks/{{this.id}}">View Tracks</a>

            </div>
        {{/each}}
        {{!-- {{> beerPartial this}} --}}            
    </div>
{{/if}}